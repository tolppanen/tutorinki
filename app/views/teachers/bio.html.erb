

<div>
  <% @skills.each do |skill| %>
    <div class="center">
      <div class="divider"></div>
      <div class="section">
              <h5><%= skill.subject.name.capitalize %> - <%= skill.subject.detail.capitalize %></h5>
              <%= link_to("Poista", skill_path(skill), :method => "delete") %>
            </div>
          </div>
  <% end %>
</div>

<%= form_for @newskill do |s| %>

<div id="prefetch">
  <%= s.text_field :subject, placeholder: "Aine", class: "typeahead", placeholder: "Aine" %>
</div>
  <%= s.submit "Lis채채", :type => "submit" %>
<% end %>

<div class="profile-edit-view">
  <%= form_for(:user, controller: "teacher", action: "save", method: "post" ) do |f| %>
  <%= devise_error_messages! %>

  <div class="field">
    <%= f.label :bio%><br />
    <%= f.text_field :bio, autocomplete: "off", :value => @teacher.bio %>
  </div>

  <div class="field">
    <%= f.label :city %><br />
    <div id="prefetch_city">
      <%= f.text_field :city, autocomplete: "off", class: "typeahead_city", :value => @teacher.city %>
    </div>
  </div>

  <div class="actions">
    <%= f.submit "P채ivit채 tiedot", :class => 'waves-effect waves-light btn'%>
  </div>

<% end %>
</div>
